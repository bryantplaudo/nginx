upstream apiklik{
	server 172.31.13.234:5000;
}
upstream apifood{
	server 172.31.13.234:5001;
}
upstream foodgcp{
	server 172.31.46.58:5000;
}

server{
	listen 80;
	location /klik/ {
		proxy_pass http://apiklik/;
	}
	
	location /food/ {
		proxy_pass http://apifood/;
	}

	location /foodgcp/ {
		proxy_pass http://foodgcp/;
	}

}
